{
  "name": "@dinamonetworks/hsm-dinamo",
  "version": "4.17.0",
  "description": "Dinamo Client NodeJS to use with DINAMO's HSMs.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles --forceExit --collectCoverage",
    "lint": "eslint . --ext .ts",
    "doc:examples": "npm run build:install && ts-node scripts/gen_examples.ts",
    "doc": "ts-node scripts/gen_examples.ts -- skip-test && typedoc --sort documents-last && ts-node scripts/add_extra_js_to_docs.ts && ts-node scripts/add_extra_head_tags.ts && ts-node scripts/change_texts.ts && ts-node scripts/post_doc_gen.ts",
    "doc:watch": "typedoc --watch",
    "doc:serve": "http-server docs -c -1",
    "build": "ts-node scripts/pre_build.ts && tsc",
    "build:install": "npm run build && ts-node scripts/install_from_build.ts"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "dinamo",
    "dinamonetworks",
    "hsm",
    "client",
    "security",
    "cryptography",
    "blockchain"
  ],
  "author": {
    "name": "Dinamo Networks",
    "email": "suporte@dinamonetworks.com",
    "url": "https://www.dinamonetworks.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "cheerio": "^1.0.0-rc.12",
    "eslint": "^9.25.1",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "crypto-js": "^4.2.0"
  },
  "jest-junit": {
    "suiteName": "HSM Dinamo NodeJS Suite Tests",
    "outputDirectory": ".",
    "outputName": "junit.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  }
}
